{
  "presets": [
    "next/babel"
  ],
  "env": {
    "development": {
      "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
      ],
      "plugins": [
        "react-intl"
      ]
    },
    "production": {
      "presets": [
        "next/babel",
        "@zeit/next-typescript/babel"
      ],
      "plugins": [
        [
          "react-intl",
          {
            "messagesDir": "static/lang/.messages/"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ],
        "@zeit/next-typescript/babel"
      ],
      "plugins": [
        [
          "react-intl",
          {
            "messagesDir": "static/lang/.messages/"
          }
        ]
      ]
    }
  }
}
